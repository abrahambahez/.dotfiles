@article{martinezespinola&alvarado2022,
  title = {{Entre coaliciones, articulaciones y difracciones: hacia una pedagog{\'i}a feminista}},
  shorttitle = {{Entre coaliciones, articulaciones y difracciones}},
  author = {Mart{\'i}nez Esp{\'i}nola, Mar{\'i}a Victoria and Alvarado, Mariana},
  year = {2022},
  month = mar,
  publisher = {{Universidad Aut{\'o}noma de Manizales}},
  issn = {2248-6941},
  doi = {10.30854/anf.v29.n52.2022.825},
  urldate = {2023-05-09},
  abstract = {Objetivo: este escrito busca dar cuenta del camino experienciado desde ciertas claves epist{\'e}mico-metodol{\'o}gicas, y a partir de ciertas pr{\'a}cticas situadas en/desde los feminismos del Sur. Toma como punto de partida acciones de escrache hacia varones cis como enclave del an{\'a}lisis de sus testimonios para dar cuenta de los m{\'u}ltiples espacio-tiempos que fuimos precisando durante la investigaci{\'o}n, a fin de dar materialidad a una epistemolog{\'i}a de las coaliciones; en la que la traducci{\'o}n, la narrativa ficcionada y el pensar en conversaci{\'o}n una junta a les otres, resultaron los dispositivos centrales. Metodolog{\'i}a: como eje de esta metodolog{\'i}a, las narrativas ficcionadas, a partir de testimonios propiciaron conversaciones, discusiones, debates, coaliciones y difractaron una elaboraci{\'o}n colectiva de materiales que habilita a pensar estrategias pedag{\'o}gicas en clave feminista. Algunas de las preguntas de indagaci{\'o}n que atraviesan y se traman en este escrito refieren a: {\textquestiondown}cu{\'a}l ha sido la renovaci{\'o}n metodol{\'o}gica que propician los feminismos del sur?, {\textquestiondown}qu{\'e} tensiones, conflictos, emergencias, cegueras y sorderas epistemol{\'o}gicas visibilizan los feminismos del sur?, {\textquestiondown}en qu{\'e} medida el desplazamiento sujeto-objeto a sujeta-sujetxs propicia otras formas de producci{\'o}n de conocimiento?, {\textquestiondown}cu{\'a}les riesgos corremos si incluimos a nuevas masculinidades como informantes para pensar los escraches?, {\textquestiondown}cu{\'a}les son las variaciones que habilita la narrativa ficcionada?, {\textquestiondown}por qu{\'e} su potencia productiva nos corre de la representaci{\'o}n a la difracci{\'o}n?, {\textquestiondown}en qu{\'e} momento asumimos una pedagog{\'i}a del deseo como anclaje creativo?, {\textquestiondown}por qu{\'e} esa pedagog{\'i}a desbordar{\'i}a los canales conocidos de toda pedagog{\'i}a posible?, {\textquestiondown}c{\'o}mo esas difracciones posibilitar{\'i}an una epistemolog{\'i}a de las coaliciones? Resultados: en el marco de este itinerar devino una pedagog{\'i}a feminista sustentada en una epistemolog{\'i}a de la localizaci{\'o}n, del posicionamiento y de las articulaciones.},
  copyright = {info:eu-repo/semantics/openAccess},
  langid = {spanish},
  annotation = {Accepted: 2023-02-03T16:20:04Z}
}

