@article{agarwal&al2021,
  title = {Retrieval {{Practice Consistently Benefits Student Learning}}: A {{Systematic Review}} of {{Applied Research}} in {{Schools}} and {{Classrooms}}},
  shorttitle = {Retrieval {{Practice Consistently Benefits Student Learning}}},
  author = {Agarwal, Pooja K. and Nunes, Ludmila D. and Blunt, Janell R.},
  year = {2021},
  month = dec,
  journal = {Educational Psychology Review},
  volume = {33},
  number = {4},
  pages = {1409--1453},
  issn = {1040-726X, 1573-336X},
  doi = {10.1007/s10648-021-09595-9},
  urldate = {2023-10-10},
  abstract = {Given the growing interest in retrieval practice among educators, it is valuable to know when retrieval practice does and does not improve student learning{\textemdash}particularly for educators who have limited classroom time and resources. In this literature review, we developed a narrow operational definition for ``classroom research'' compared to previous reviews of the literature. We screened nearly 2000 abstracts and systematically coded 50 experiments to establish a clearer picture of benefits from retrieval practice in real world educational settings. Our review yielded 49 effect sizes and a total n = 5374, the majority of which (57\%) revealed medium or large benefits from retrieval practice. We found that retrieval practice improved learning for a variety of education levels, content areas, experimental designs, final test delays, retrieval and final test formats, and timing of retrieval practice and feedback; however, only 6\% of experiments were conducted in nonWEIRD countries. Based on our review of the literature, we make eight recommendations for future research and provide educators with a better understanding of the robust benefits of retrieval practice across a range of school and classroom settings.},
  langid = {english}
}

