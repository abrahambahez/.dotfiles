@article{vargheese&al2016,
  title = {{Constructing Social Media Knowledge Graphs with Social Scientists}},
  author = {Vargheese, John Paul and Travers, Peter and Pan, Jeff and Vincent, Kathryn and Wallace, Claire and Kabedeva, Anna},
