@article{barajas2019,
  title = {{Desaf{\'i}os de la etnograf{\'i}a digital en el trabajo de campo onlife}},
  author = {Barajas, Karina B{\'a}rcenas and Carre{\~n}o, Nohem{\'i} Preza},
